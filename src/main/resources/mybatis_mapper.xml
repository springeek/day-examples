<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="demo.sql">
    <insert id="insertStudent" parameterType="com.spk.mybatis.Student">
        insert into users(name,password,age,birthday,email) values(#{name},#{password},#{age},#{birthday},#{email})
    </insert>
</mapper>