<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <!--<PatternLayout pattern="\r\n>[%-5level] %t %l\r\n%d{yyyyMMddHHmmss} %msg%n" />-->
            <PatternLayout pattern="\r\n>[%-5level] %t %l\r\n%d{yyyyMMddHHmmss} %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <logger name="demo.sql" level="debug" additivity="false">
            <AppenderRef ref="Console"/>
        </logger>
        <Root level="error">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>